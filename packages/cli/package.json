{
  "name": "@mirror-dissonance/cli",
  "version": "1.0.0",
  "type": "module",
  "description": "CLI wrapper for Mirror Dissonance Protocol",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "oracle": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "postbuild": "node -e \"const {cpSync,mkdirSync}=require('fs');const {join}=require('path');mkdirSync('dist/schemas',{recursive:true});cpSync('../mirror-dissonance/schemas/dissonance-report.schema.json','dist/schemas/dissonance-report.schema.json')\"",
    "start": "node dist/index.js",
    "clean": "rm -rf dist",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "echo \"Linting not yet implemented\" && exit 0"
  },
  "dependencies": {
    "@mirror-dissonance/core": "workspace:*",
    "boxen": "^7.1.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "glob": "^10.3.10",
    "inquirer": "^9.2.12",
    "js-yaml": "^4.1.0",
    "ora": "^7.0.1",
    "table": "^6.8.1"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.11",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.0"
  },
  "license": "Apache-2.0"
}
